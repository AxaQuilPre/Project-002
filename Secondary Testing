class sorter:
    def __init__(self,first,last,eyevision) -> None:
        self.first = first
        self.last = last
        self.eyevis = eyevision
        self.fullname = first + " " + last
    def checker(self):
        if self.eyevis > 4:
            self.eyestatus = "Good"
        else:
            self.eyestatus = "Bad"
        return self.eyestatus


emp_1 = sorter("AurÃ©lien", "Jacot",5)
print(sorter.checker(emp_1))